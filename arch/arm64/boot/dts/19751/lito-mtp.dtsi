&qupv3_se0_i2c {
//#ifdef VENDOR_EDIT
// zhangyuwei@MM.Display.LCD.Machine 2019-10-14
	status = "ok";
	qcom,clk-freq-out = <400000>;
//#else
//	status = "disabled";
//	qcom,clk-freq-out = <1000000>;
//#endif
	#address-cells = <1>;
	#size-cells = <0>;
	nq@28 {
		compatible = "qcom,nq-nci";
		reg = <0x28>;
		qcom,nq-irq = <&tlmm 34 0x00>;
		qcom,nq-ven = <&tlmm 12 0x00>;
		qcom,nq-firm = <&tlmm 35 0x00>;
		qcom,nq-clkreq = <&tlmm 58 0x00>;
		interrupt-parent = <&tlmm>;
		interrupts = <34 0>;
		interrupt-names = "nfc_irq";
		pinctrl-names = "nfc_active", "nfc_suspend";
		pinctrl-0 = <&nfc_int_active &nfc_enable_active
				&nfc_clk_req_active>;
		pinctrl-1 = <&nfc_int_suspend &nfc_enable_suspend
				&nfc_clk_req_suspend>;
	};

//#ifdef VENDOR_EDIT
// zhangyuwei@MM.Display.LCD.Machine 2019-10-14 add nodes for backlight and lcd power
    ti_lm3697_bl@36 {
	    compatible = "lm3697";
        reg = <0x36>;
        ti,bl-enable-gpio = <&pm8150l_gpios 2 0>;
    };

    ti_tps65132_pw@3e {
        compatible = "tps65132";
        reg = <0x3e>;
        status = "okay";
